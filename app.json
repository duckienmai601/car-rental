{
  "expo": {
    "name": "rent-a-car",
    "slug": "rent-a-car",
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "permissions": {
      "cameraRoll": {
        "ios": "Allow $(PRODUCT_NAME) to access your photos",
        "android": "Allow $(PRODUCT_NAME) to access your photos"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
